{
  "DatabaseProvider": "SQLite",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=DigitalAssistantServer.db"
  },
  "PreventLoadingAiModels": false,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "/etc/ssl/digitalassistant/ssl.crt",
        "KeyPath": "/etc/ssl/digitalassistant/ssl.key"
      }
    }
  },
  "ClientConnection": {
    "Port": 59142,
    "Certificate": {
      "Path": "/etc/ssl/digitalassistant/ssl.crt",
      "KeyPath": "/etc/ssl/digitalassistant/ssl.key"
    }
  },
  "FileStorePath": "./DigitalAssistantServer/DigitalAssistantServer_FileStore/",
  "TempFileStorePath": "./DigitalAssistantServer/DigitalAssistantServer_FileStore/Temp/",
  "ModelsDirectoryPath": "./DigitalAssistantServer/Models/"
}